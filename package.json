{
  "name": "react-styled-classnames",
  "version": "0.1.5",
  "author": "https://github.com/richard-unterberg",
  "repository": {
    "type": "git",
    "url": "https://github.com/richard-unterberg/react-styled-classnames.git"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwind",
    "unocss",
    "dynamic-classnames",
    "utility-first",
    "classnames",
    "frontend",
    "css"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "build": "npm run build-all",
    "build-all": "rm -rf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "package": "npm run build && npm publish --access public",
    "test": "jest",
    "type-test": "tsd"
  },
  "files": [
    "dist/**/*"
  ],
  "peerDependencies": {
    "clsx": "^1.2.1",
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.11",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsd": "^0.31.2",
    "tsx": "^4.19.2"
  }
}
