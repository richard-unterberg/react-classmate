{
  "name": "react-dynamic-classnames",
  "version": "0.0.12",
  "author": "https://github.com/richard-unterberg",
  "repository": {
    "type": "git",
    "url": "https://github.com/richard-unterberg/react-dynamic-classnames.git"
  },
  "keywords": [
    "react",
    "typescript",
    "dynamic-classnames",
    "utility-first",
    "frontend",
    "css"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
"scripts": {
  "build": "npm run build-all",
  "build-all": "tsc && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
  "package": "npm run build && npm publish --access public"
},
  "files": [
    "dist/**/*"
  ],
  "peerDependencies": {
    "clsx": "^1.2.1",
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.11"
  }
}
